<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        x:Class="OneDbgClient.MainWindow"
        Title="OneDbg" Height="300" Width="300"
        DataContext="{Binding MainWindow, Mode=OneWay, Source={StaticResource Locator}}"
        xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
        xmlns:b="clr-namespace:OneDbgClient.Behaviors"
        xmlns:views="clr-namespace:OneDbgClient.Views">
    <Grid>
        <TabControl>
            <i:Interaction.Behaviors>
                <b:DebugProcessesTabBehavior DebugProcesses="{Binding DebugProcesses}"/>
            </i:Interaction.Behaviors>
            <TabItem Header="Processes">
                <views:ProcessesView/>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
